{
  "manifest_version": 2,

  "name": "De-distract myself",
  "description": "Block certain pages",
  "version": "2.0",

  "content_scripts": [
    {
      "matches": [
          "https://*.reddit.com/*",
          "https://*.youtube.com/*",
          "https://*.facebook.com/*",
          "https://*.twitter.com/*",
          "https://*.telex.hu/*",
          "https://*.hvg.hu/*",
          "https://*.pinterest.com/*",
          "https://*.wikipedia.org/*",
          "https://*.linkedin.com/*",
          "https://*.gmail.com/*",
          "https://mail.google.com/*"
      ],
      "css": ["shared.css"],
      "js": ["shared.js"],
      "run_at": "document_start"
    }
  ]
}
